package Domini;

public class PartidaDom {
	
	//Atribus de partida
	private int forma;
	private int numCas;
	private int numCol;
	private int agujeros;
	
	/*Constructores*/
	public void crearPartPers(int forma, int numCas, int numCol, int agujeros){
		forma = this.forma;
		numCas = this.numCas;
		numCol = this.numCol;
		agujeros = this.agujeros;
	}
	
	public void crearPartDef(){
		forma =(int)(Math.random()*(3-1))+1;
		numCas = (int)(Math.random()*(100-20))+20;
		//numCol = 
		//agujero = etc etc
	}
}
